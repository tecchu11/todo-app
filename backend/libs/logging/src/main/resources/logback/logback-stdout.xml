<?xml version="1.0" encoding="UTF-8" ?>

<included>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%d{yyyy-MM-dd'T'HH:mm:ss.SSSXXX} %-5p [%t] %lo{20} %X -- %msg%n</pattern>
        </encoder>
    </appender>
</included>
